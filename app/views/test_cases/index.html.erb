<div class="row">
    <div class="nine panel columns centered">
      <h1>Listing test_cases</h1>

      <%= link_to 'New Test case', new_test_case_path %>
      <br /><br />

      <table>
        <thead>
        <tr>
          <th>Input</th>
          <th>Output</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
        </thead>
        <tbody>
          <% @test_cases.each do |test_case| %>
            <tr>
              <td><%= test_case.input %></td>
              <td><%= test_case.output %></td>
              <td><%= link_to 'Show', test_case %></td>
              <td><%= link_to 'Edit', edit_test_case_path(test_case) %></td>
              <td><%= link_to 'Destroy', test_case, confirm: 'Are you sure?', method: :delete %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br />
    </div>
</div>